
<ul class="row list-unstyled wb-eqht-grd wb-filter mrgn-tp-md pb-4 wb-init wb-filter-inited">
		{% for event in eventslist %}
		<li class="col-xs-12 col-md-6 mrgn-tp-md mrgn-bttm-md">
			<div class="brdr-tp brdr-rght brdr-bttm brdr-lft hght-inhrt">
				<h3 class="mrgn-tp-md mrgn-rght-md mrgn-bttm-md mrgn-lft-md">{{ event.data.title }}
					{% if event.data.online %}<span class="label label-success mrgn-lft-sm"><span class="wb-inv">{{ events[locale].location }} </span>{{ events[locale].online }}</span>{% endif %}
					{% if event.data.host %}<small>{{ events[locale].hostedBy }} <strong>{{ event.data.host }}</strong></small>{% endif %}
				</h3>
				<div class="mrgn-rght-md mrgn-bttm-md mrgn-lft-md">
					<dl>
						<dt>{{ events[locale].eventDate }}</dt>
						<dd>{{ event.data.eventDate | htmlDateString }}</dd>
						<dt>{{ events[locale].eventDescription }}</dt>
						<dd>{{ event.data.description }}</dd>
					</dl>
				</div>
			</div>
		</li>
		{% endfor %}
</ul>
